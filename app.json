{
  "expo": {
    "name": "Cards",
    "slug": "Cards",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/check.png",
    "splash": {
      "image": "./assets/check.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffcc"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": ".eduarda.dalmas.cards",
      "supportsTablet": true
    },
    "android": {
      "package": ".eduarda.dalmas.cards"
    }
  }
}
